create table github_repos (
    added_at datetime,
    id int,
    repo_name varchar(255),
    description varchar(255),
    author_id int,
    author_name varchar(255),
    created_at datetime,
    last_modified datetime,
    language varchar(255),
    stargazers_count int,
    forks_count int,
    from_hacker_news tinyint);
